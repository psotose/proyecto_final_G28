<p id="notice"><%= notice %></p>

<h1>Real Estate Brokers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Rut</th>
      <th>Agent</th>
      <th>Mail</th>
      <th>Phone</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_estate_brokers.each do |real_estate_broker| %>
      <tr>
        <td><%= real_estate_broker.name %></td>
        <td><%= real_estate_broker.rut %></td>
        <td><%= real_estate_broker.agent %></td>
        <td><%= real_estate_broker.mail %></td>
        <td><%= real_estate_broker.phone %></td>
        <td><%= real_estate_broker.image %></td>
        <td><%= link_to 'Show', real_estate_broker %></td>
        <td><%= link_to 'Edit', edit_real_estate_broker_path(real_estate_broker) %></td>
        <td><%= link_to 'Destroy', real_estate_broker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real Estate Broker', new_real_estate_broker_path %>
